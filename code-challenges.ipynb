# Two Sum
nums = [2,5,5,11]

target = 10

def twoSum(arr, target):
    d = {}

    for num in arr:
        m = target - num

        if m in d:
            return [d[m], num]
        else:
            d[num] = num

print(twoSum(nums, target))
