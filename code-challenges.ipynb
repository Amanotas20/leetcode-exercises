# Custom Sort String
def customSortString(order, str):
    from collections import Counter

    d = {v:k for k, v in enumerate(order)}
    outputString = ''

    cc = Counter([char for char in string])

    for val in cc:
        d[val] = cc[val]

    for key in d:
        if key in cc:
            outputString += key*d[key]
            
    return outputString
        


