# Find Peak Element
def findPeakElement(nums):
    if (nums[0] > nums[1]):
        return 0
    elif (nums[len(nums)-1] > nums[len(nums)-2]):
        return len(nums)-1
    else:
        for i in range(1, len(nums)-1):        
            if (nums[i-1] < nums[i] and nums[i+1] < nums[i] and nums[i] != nums[i+1]):
                return i

